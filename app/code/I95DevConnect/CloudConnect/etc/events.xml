<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="push_data_after_subscription_success">
        <observer name="get_cloud_shipping_list_forward"
                  instance="I95DevConnect\CloudConnect\Observer\ShippingMappingList"/>
        <observer name="get_cloud_payment_list_forward"
                  instance="I95DevConnect\CloudConnect\Observer\PaymentMappingList"/>
    </event>
    <event name="pull_data_after_subscription_success">
        <observer name="get_cloud_shipping_list_reverse"
                  instance="I95DevConnect\CloudConnect\Observer\ShippingMappingList"/>
        <observer name="get_cloud_payment_list_reverse"
                  instance="I95DevConnect\CloudConnect\Observer\PaymentMappingList"/>
    </event>
</config>
