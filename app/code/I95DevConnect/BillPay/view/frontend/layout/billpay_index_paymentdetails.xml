<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="I95DevConnect_Billpay::css/billpay.css" />
    </head>
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="I95DevConnect\BillPay\Block\Customer\PaymentDetails" name="billpay.customer.paymentdetails" cacheable="false">
                <block class="I95DevConnect\BillPay\Block\Customer\PaymentDetails\PaymentDetails" name="payment_details" as="payment_details"/>
                <block class="I95DevConnect\BillPay\Block\Customer\PaymentDetails\TransactionDetails" name="transaction_details" as="transaction_details"/>
                <block class="I95DevConnect\BillPay\Block\Customer\PaymentDetails\ListPayment" name="billpay.customer.paymentlist" as="listpayment"/>
            </block>
        </referenceContainer>
    </body>
</page>
