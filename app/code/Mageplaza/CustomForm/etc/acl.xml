<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
                    <resource id="Mageplaza_CustomForm::custom_form" title="Custom Form" translate="title" sortOrder="40">
                        <resource id="Mageplaza_CustomForm::form" title="Manage Forms" translate="title" sortOrder="20"/>
                        <resource id="Mageplaza_CustomForm::responses" title="Manage Responses" translate="title" sortOrder="30"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Mageplaza_CustomForm::configuration" title="Mageplaza Custom Form"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
